{
  "name": "PHPNinja/skeleton",
  "type": "project",
  "license": "MIT",
  "description": "Squelette pour cr√©ation d'une web application avec PHPNinja",
  "minimum-stability": "dev",
  "autoload" : {
      "psr-4" : {
          "App\\" : "src/"     
      }
  },
  "repositories":[
      {
          "type": "package",
          "package": {
              "name": "uriostachi/phpninja",
              "version" : "1.1",
              "source": {
                  "url": "https://github.com/Uriostachi/phpninja.git",
                  "type": "git",
                  "reference": "master"
              },
              "autoload": {
                  "psr-4": { "PHPNinja\\": ["src/"] }
              }
          }
      }
  ],
  "require": {
        "uriostachi/phpninja": "^1.0",
        "twig/twig": "^3.0",
        "nikic/fast-route": "^1.3" 
  }
}